[global_config]
  title_hide_sizetext = True
  title_transmit_bg_color = "#5e2750"
  title_inactive_fg_color = "#babdb6"
  title_inactive_bg_color = "#555753"
[keybindings]
[profiles]
  [[default]]
    background_color = "#333333"
    cursor_color = "#aaaaaa"
    foreground_color = "#eeeeec"
    scrollback_infinite = True
    palette = "#000000:#ef2929:#73d216:#c4a000:#729fcf:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    copy_on_selection = True
[layouts]
  [[default]]
    [[[child0]]]
      type = Window
      parent = ""
      order = 0
      position = 400:75
      maximised = False
      fullscreen = False
      size = 1200, 850
    [[[terminal1]]]
      type = Terminal
      parent = child0
      order = 0
      profile = default
[plugins]